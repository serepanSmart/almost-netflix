{
  "name": "react-course",
  "version": "1.0.0",
  "description": "react study",
  "private": true,
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "dev": "next dev -p 4200",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --color",
    "prepare": "husky install",
    "storybook": "start-storybook -p 4100",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serepanSmart/react-course.git"
  },
  "keywords": [
    "js",
    "javascript"
  ],
  "resolutions": {
    "styled-components": "^5"
  },
  "author": "Serhii Vovna, <Serhii_Vovna@epam.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/serepanSmart/react-course/issues"
  },
  "homepage": "https://github.com/serepanSmart/react-course#readme",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.17.12",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.34",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "hot-module-replacement": "^3.0.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-styled-components": "^7.0.8",
    "less": "^4.1.2",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "2.6.2",
    "react-html-props": "^1.0.32",
    "react-select-event": "^5.5.0",
    "redux-devtools-extension": "^2.13.9",
    "server": "^1.0.37",
    "storybook-addon-jsx": "^7.3.14",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "typescript-plugin-styled-components": "^2.0.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "@datepicker-react/styled": "^2.8.4",
    "@reduxjs/toolkit": "^1.8.2",
    "@styled-icons/bootstrap": "^10.34.0",
    "@types/react-datepicker": "^4.4.1",
    "babel-jest": "^28.1.2",
    "formik": "^2.2.9",
    "moment": "^2.29.3",
    "next": "^12.2.2",
    "react": "^18.1.0",
    "react-datepicker": "^4.8.0",
    "react-datepicker-styled-components": "^1.0.4",
    "react-day-picker": "^8.0.5",
    "react-dom": "^18.1.0",
    "react-modal": "^3.15.1",
    "react-redux": "^8.0.2",
    "react-select": "^5.3.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "styled-bootstrap-grid": "^3.1.2",
    "styled-components": "^5.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "eslint --cache --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "moduleDirectories": [
      "node_modules",
      "src",
      "assets"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageProvider": "babel",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^@$": "<rootDir>/src",
      "^@/(.*)": "<rootDir>/src/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest_config/setupTest.js",
      "@testing-library/jest-dom/extend-expect"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!lodash-es/).+\\.js$"
    ]
  }
}
